class Solution{ 
public:
    string onesComplement(string S,int N)
    {
        string ans = "";
        for(int i = 0; i < N; i++)
        {
            if(S[i] == '1')
            {
                ans += "0";
            }
            else
            {
                ans += "1";
            }
        }
        return ans;
    }
};
tc o(n) sc o(1)
