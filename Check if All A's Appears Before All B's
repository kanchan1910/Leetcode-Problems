class Solution {
public:
    bool checkString(string s)
    {
        bool boccurs = false;
        int n = s.length();
        for(int i = 0; i < n; i++)
        {
            if(s[i] == 'b')
            {
                boccurs = true;
            }
            if(boccurs == true)
            {
                if(s[i] == 'a')
                {
                    return false;
                }
            }
        }
        return true;
    }
};
tc o(n) sc o(1)
